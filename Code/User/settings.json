{
    "debug.console.fontFamily": "Fira Code",
    "debug.console.wordWrap": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": false
    },
    "editor.dragAndDrop": false,
    "editor.find.cursorMoveOnType": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.fontWeight": 500,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.rulers": [
        80,
        100,
        120
    ],
    "editor.suggestSelection": "first",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "rainbow1",
                "settings": {
                    "foreground": "#E6194B"
                }
            },
            {
                "scope": "keyword.rainbow2",
                "settings": {
                    "foreground": "#3CB44B"
                }
            },
            {
                "scope": "entity.name.function.rainbow3",
                "settings": {
                    "foreground": "#FFE119"
                }
            },
            {
                "scope": "comment.rainbow4",
                "settings": {
                    "foreground": "#0082C8"
                }
            },
            {
                "scope": "string.rainbow5",
                "settings": {
                    "foreground": "#FABEBE"
                }
            },
            {
                "scope": "variable.parameter.rainbow6",
                "settings": {
                    "foreground": "#46F0F0"
                }
            },
            {
                "scope": "constant.numeric.rainbow7",
                "settings": {
                    "foreground": "#F032E6"
                }
            },
            {
                "scope": "entity.name.type.rainbow8",
                "settings": {
                    "foreground": "#008080"
                }
            },
            {
                "scope": "markup.bold.rainbow9",
                "settings": {
                    "foreground": "#F58231"
                }
            },
            {
                "scope": "invalid.rainbow10",
                "settings": {
                    "foreground": "#FFFFFF"
                }
            }
        ]
    },
    "explorer.confirmDelete": false,
    "files.trimTrailingWhitespace": true,
    "gitlens.defaultDateFormat": "YYYY-MM-DD h:mma",
    "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
    "search.showLineNumbers": true,
    "terminal.integrated.profiles.windows": {
        "PowerShell -NoProfile": {
          "source": "PowerShell",
          "args": ["-NoProfile"]
        },
        "Windows PowerShell": {
          "path": "powershell.exe",
          "args": [""]
        }
    },
    "terminal.integrated.defaultProfile.windows": "PowerShell -NoProfile",
    // "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Dracula",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },

    "azureFunctions.showCoreToolsWarning": false,
    "crates.localCargoIndexBranch": "origin/HEAD",
    "csharp.suppressDotnetRestoreNotification": true,
    "cSpell.enableFiletypes": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "go",
        "handlebars",
        "html",
        "jade",
        "javascript",
        "javascriptreact",
        "json",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "powershell",
        "pub",
        "python",
        "restructuredtext",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yml"
    ],
    "cSpell.ignoreRegExpList": [
        "[\"^\\\\$.+\", \"http\", \"https\"]"
    ],
    "cSpell.userWords": [
        "ardome",
        "changeset",
        "cmdlet",
        "demuxers",
        "dependee",
        "embeddable",
        "exif",
        "ffprobe",
        "flurl",
        "healthcheck",
        "interruptible",
        "parallelize",
        "pylint",
        "recurse",
        "repoll",
        "uploader",
        "vcos",
        "vizrt",
        "xunit"
    ],
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "vue"
    ],
    "hg.annotationEnabled": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "java.configuration.checkProjectSettingsExclusions": false,
    "java.home": "C:\\Program Files\\ojdkbuild\\java-11-openjdk-11.0.8-1",
    "java.semanticHighlighting.enabled": true,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "javascript.inlayHints.enumMemberValues.enabled": true,
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.inlayHints.parameterNames.enabled": "all",
    "javascript.inlayHints.parameterTypes.enabled": true,
    "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
    "javascript.inlayHints.variableTypes.enabled": true,
    "jupyter.interactiveWindowMode": "perFile",
    "omnisharp.projectLoadTimeout": 120,
    // "python.dataScience.debugJustMyCode": false,
    "python.jediEnabled": false,
    "typescript.inlayHints.enumMemberValues.enabled": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.inlayHints.parameterNames.enabled": "all",
    "typescript.inlayHints.parameterTypes.enabled": true,
    "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
    "typescript.inlayHints.variableTypes.enabled": true,
    "vetur.validation.template": false
}
