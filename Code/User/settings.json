{
    "azureFunctions.showCoreToolsWarning": false,
    "debug.console.fontFamily": "Fira Code",
    "debug.console.wordWrap": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": false
    },
    "editor.dragAndDrop": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.rulers": [
        80,
        100,
        120
    ],
    "editor.suggestSelection": "first",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "rainbow1",
                "settings": {
                    "foreground": "#E6194B"
                }
            },
            {
                "scope": "keyword.rainbow2",
                "settings": {
                    "foreground": "#3CB44B"
                }
            },
            {
                "scope": "entity.name.function.rainbow3",
                "settings": {
                    "foreground": "#FFE119"
                }
            },
            {
                "scope": "comment.rainbow4",
                "settings": {
                    "foreground": "#0082C8"
                }
            },
            {
                "scope": "string.rainbow5",
                "settings": {
                    "foreground": "#FABEBE"
                }
            },
            {
                "scope": "variable.parameter.rainbow6",
                "settings": {
                    "foreground": "#46F0F0"
                }
            },
            {
                "scope": "constant.numeric.rainbow7",
                "settings": {
                    "foreground": "#F032E6"
                }
            },
            {
                "scope": "entity.name.type.rainbow8",
                "settings": {
                    "foreground": "#008080"
                }
            },
            {
                "scope": "markup.bold.rainbow9",
                "settings": {
                    "foreground": "#F58231"
                }
            },
            {
                "scope": "invalid.rainbow10",
                "settings": {
                    "foreground": "#FFFFFF"
                }
            }
        ]
    },
    "explorer.confirmDelete": false,
    "files.trimTrailingWhitespace": true,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true,
        "suppressWelcomeNotice": true
    },
    "gitlens.defaultDateFormat": "YYYY-MM-DD h:mma",
    "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
    // For mac:
    // "gitlens.keymap": "alternate",
    "search.showLineNumbers": true,
    "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    // "terminal.integrated.rendererType": "dom",
    "terminal.integrated.rendererType": "experimentalWebgl",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Dracula",
    "workbench.editor.highlightModifiedTabs": true,

    "csharp.suppressDotnetRestoreNotification": true,
    "cSpell.enableFiletypes": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "go",
        "handlebars",
        "html",
        "jade",
        "javascript",
        "javascriptreact",
        "json",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "powershell",
        "pub",
        "python",
        "restructuredtext",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yml"
    ],
    "cSpell.userWords": [
        "ardome",
        "changeset",
        "cmdlet",
        "demuxers",
        "dependee",
        "embeddable",
        "exif",
        "ffprobe",
        "flurl",
        "healthcheck",
        "interruptible",
        "parallelize",
        "pylint",
        "recurse",
        "repoll",
        "uploader",
        "vcos",
        "vizrt",
        "xunit"
    ],
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "vue"
    ],
    "hg.annotationEnabled": true,
    "java.configuration.checkProjectSettingsExclusions": false,
    "java.home": "C:\\Program Files\\ojdkbuild\\java-1.8.0-openjdk-1.8.0.212-1",
    "java.semanticHighlighting.enabled": true,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "omnisharp.projectLoadTimeout": 120,
    "python.jediEnabled": false,
    "vetur.validation.template": false
}
